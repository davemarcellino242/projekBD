<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clubApp.RegistrasiKegiatan">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/g4.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/g2.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="150.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Image/g3.png" />
         </image>
      </ImageView>
      <TextArea fx:id="data" layoutX="166.0" layoutY="160.0" prefHeight="150.0" prefWidth="200.0" />
      <Label layoutX="193.0" layoutY="122.0" prefHeight="34.0" prefWidth="126.0" style="-fx-background-color: #F9F6EE;" text="Kode kegiatan club">
         <font>
            <Font name="Bell MT Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="notification" layoutX="185.0" layoutY="337.0" prefHeight="27.0" prefWidth="141.0" textFill="#fffafa">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="411.0" layoutY="143.0" prefHeight="34.0" prefWidth="150.0" style="-fx-background-color: #F9F6EE;" text="Masukkan kode kegiatan club : " textAlignment="CENTER" wrapText="true" />
      <Button layoutX="460.0" layoutY="276.0" mnemonicParsing="false" onAction="#submitHandle" text="Submit" />
      <Button layoutX="535.0" layoutY="364.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <ComboBox fx:id="kodeKegiatan" layoutX="411.0" layoutY="188.0" prefWidth="150.0" />
   </children>
</AnchorPane>
